{
  "name": "data-view-vue3",
  "version": "0.0.4",
  "description": "DataV of Vue3",
  "private": false,
  "license": "MIT",
  "main": "dist/lib.umd.js",
  "module": "dist/lib.es.js",
  "style": "dist/style.css",
  "type": "module",
  "exports": {
    ".": {
      "import": "dist/lib.es.js",
      "require": "dist/lib.umd.js"
    },
    "./*": "./*"
  },
  "scripts": {
    "start": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:prod": "vue-tsc --noEmit && vite build --mode production",
    "preview": "vite preview --prot 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx"
  },
  "keywords": [
    "vue3",
    "vite",
    "component",
    "ui",
    "datav",
    "data-view",
    "typescript"
  ],
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/node": "^17.0.22",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "dart-sass": "^1.25.0",
    "eslint": "^8.11.0",
    "eslint-plugin-vue": "^8.5.0",
    "sass": "^1.49.9",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vue-tsc": "^0.29.8"
  },
  "homepage": "https://github.com/maybeyourking/data-view-vue3",
  "bugs": {
    "url": "https://github.com/maybeyourking/data-view-vue3/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maybeyourking/data-view-vue3"
  }  
}
